[
  {
    "name": "Custom Property",
    "id": "com.camunda.example.CustomProperty",
    "appliesTo": ["bpmn:UserTask"],
    "isDefault": true,
    "properties": [
      {
        "label": "Completed If",
        "type": "String",
        "binding": {
          "type": "camunda:property",
          "name": "completedIf",
          "value": "${ userInput }"
        }
      },
      {
        "label": "Buttons",
        "type": "String",
        "binding": {
          "type": "camunda:property",
          "name": "buttons",
          "value": "${ userInput }"
        }
      }
    ],
    "entriesVisible": {
      "_all": true
    }
  }
]
